{
	"github": {
		"silent": true
	},
	"cleanUrls": true,
	"trailingSlash": false,
	"rewrites": [
		{ "source": "/api/webpush/(.*)", "destination": "/api/webpush" },
		{ "source": "/api/pvt/(.*)", "destination": "/api/pvt" }
	],
	"redirects": [{ "source": "/sw.js", "destination": "/service-worker.js" }]
}
